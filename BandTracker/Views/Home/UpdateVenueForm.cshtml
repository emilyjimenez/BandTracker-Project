@{
    Layout = "_Layout";
}
<h1>Band Tracker!</h1>
<h2>Add new venue</h2>
<form action='/venues/@Model.Id/update' method="post">
  <label for="update-name">Venue name:</label>
  <input id="update-name" name="update-name" type="text">
  <label for="update-address">Venue City and State:</label>
  <input id="update-address" name="update-address" type="text">
  <button type="submit">Update</button>
</form>
<h4><a href="/">Return home</a></h4>
