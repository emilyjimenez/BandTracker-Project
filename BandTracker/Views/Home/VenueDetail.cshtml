@{
  Layout = "_Layout";
}
<div class="container">
  <h1>Band Tracker</h1>
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venues/venue.Id'>@venue.Name</a></li>
    }
  </ul>
}
<h4><a href="/venues/new">Add a new venue</a></h4>
<h4><a href="/">Return home</a></h4>
</div>
